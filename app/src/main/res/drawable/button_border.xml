<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/aapt"
            xmlns:Android="http://schemas.android.com/apk/res/android">
    <item>
        <shape Android:shape="rectangle">
            <solid Android:color="@color/check_upgrade_bg"/>
            <corners Android:radius="19dp"/>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"/>
        </shape>
    </item>

    <item Android:top="1px">
        <shape Android:shape="rectangle">

            <padding
                Android:bottom="3dp"
                Android:left="5dp"
                android:right="5dp"
                android:top="3dp"/>
            <corners Android:radius="20dp"/>
        </shape>

    </item>

</layer-list>
